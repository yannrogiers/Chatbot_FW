{
  "name": "chatbot_react_dialogflow",
  "version": "1.0.0",
  "description": "React bot for webpage",
  "main": "index.js",
  "engines": {
    "node": "13.6.0",
    "npm": "6.14.5"
  },
  "scripts": {
    "start": "node index.js",
    "backend": "nodemon index.js",
